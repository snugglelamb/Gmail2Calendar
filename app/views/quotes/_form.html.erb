<%= form_for @quote, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :user_id, :class => 'control-label' %>
    <div class="controls">
      <!-- <%= f.number_field :user_id, :class => 'number_field' %> -->
	  <%= collection_select :quote, :user_id, User.all, :id, :name%>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :review, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :review, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                quotes_path, :class => 'btn' %>
  </div>
<% end %>
