<p id="notice"><%= notice %></p>

<h1>Listing Mygmails</h1>

<table>
  <thead>
    <tr>
      <th>Eid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mygmails.each do |mygmail| %>
      <tr>
        <td><%= mygmail.eid %></td>
        <td><%= link_to 'Show', mygmail %></td>
        <td><%= link_to 'Edit', edit_mygmail_path(mygmail) %></td>
        <td><%= link_to 'Destroy', mygmail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mygmail', new_mygmail_path %>
