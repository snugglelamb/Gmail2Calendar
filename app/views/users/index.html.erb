<%- model_class = User -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <!-- <th><%= model_class.human_attribute_name(:psw) %></th> -->
      <!-- <th><%= model_class.human_attribute_name(:account) %></th> -->
      <th><%= model_class.human_attribute_name(:email) %></th>
      <!-- <th><%= model_class.human_attribute_name(:encrypted_password) %></th>
      <th><%= model_class.human_attribute_name(:reset_password_token) %></th>
      <th><%= model_class.human_attribute_name(:reset_password_sent_at) %></th>
      <th><%= model_class.human_attribute_name(:remember_created_at) %></th>
      <th><%= model_class.human_attribute_name(:current_sign_in_at) %></th> -->
      <th><%= model_class.human_attribute_name(:last_sign_in_at) %></th>
      <th><%= model_class.human_attribute_name(:current_sign_in_ip) %></th>
      <!-- <th><%= model_class.human_attribute_name(:last_sign_in_ip) %></th> -->
      <!-- <th><%= model_class.human_attribute_name(:provider) %></th> -->
      <th><%= model_class.human_attribute_name(:uid) %></th>
      <!-- <th><%= model_class.human_attribute_name(:created_at) %></th> -->
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.id, user_path(user) %></td>
        <td><%= user.name %></td>
        <!-- <td><%= user.psw %></td>
        <td><%= user.account %></td> -->
        <td><%= user.email %></td>
        <!-- <td><%= user.encrypted_password %></td>
        <td><%= user.reset_password_token %></td>
        <td><%= user.reset_password_sent_at %></td> -->
        <!-- <td><%= user.remember_created_at %></td> -->
        <!-- <td><%= user.current_sign_in_at %></td> -->
        <td><%= user.last_sign_in_at %></td>
        <td><%= user.current_sign_in_ip %></td>
        <!-- <td><%= user.last_sign_in_ip %></td>
        <td><%= user.provider %></td> -->
        <td><%= user.uid %></td>
        <!-- <td><%=l user.created_at %></td> -->
        <td>
			<%if user.id == current_user.id%>
				<%= link_to 'Show', user,:class => 'btn btn-mini' %>
          	  	<%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_user_path(user), :class => 'btn btn-mini' %>
          <!-- <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      user_path(user),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %> -->
			  <% end%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<% if (current_user.psw.nil? || current_user.psw.empty?) %>
	<%= link_to 'Set up Gmail Account Access', edit_user_path(current_user), :method => :get, :confirm => 'You will be directed to set up Gmail Account Access' %>
<% else %>
	<% url = user_getgmails_path(current_user.id) %>
	

	<%= form_tag(url, :method => "get") do %>
	    <%= label_tag(:q, "Please choose date:") %>
	    <%= text_field_tag(:q) %>
	    <%= submit_tag "Get my Gmails" %>
	<% end %>

<% end %>

			
	<!-- <%= link_to t('.new', :default => t("helpers.links.new")),
            new_user_path,
            :class => 'btn btn-primary' %> -->
 	
	<!-- The following code is to realize the jQuery UI datepicker feature -->	
	<script src="http://code.jquery.com/jquery-1.10.2.js"></script>
	<script src="http://code.jquery.com/ui/1.10.4/jquery-ui.js"></script>
	<script>
	  $(function() {
	     $( "#q" ).datepicker({
	          appendText:"(yy-mm-dd)",
	          dateFormat:"yy-mm-dd",
	       });
	  });
	</script>
